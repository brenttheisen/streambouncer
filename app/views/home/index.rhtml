<h1>Followers</h1>

<ul id="search-results">
	<% @logged_in_user.followed_twitter_users.each { |twitter_user| %>
		<li>
			<div class="username"><%= twitter_user.username %></div>
			<div class="name"><%= twitter_user.name %></div>
		</li>
	<% } %>
</ul>
